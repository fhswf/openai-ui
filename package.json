{
  "name": "openai-ui",
  "version": "0.17.3",
  "description": "Privacy-focused UI for OpenAI APIs",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "cypress run --headless --env TESTENV=CI --spec cypress/e2e/uiTest.cy.js",
    "test:e2e:ci": "cypress run --headless --env TESTENV=CI --spec cypress/e2e/uiTest.cy.js",
    "test:e2e:prod": "cypress run --headless --env TESTENV=PROD --spec cypress/e2e/uiTest.cy.js",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.13.0",
    "@emotion/react": "^11.14.0",
    "@uiw/react-textarea-code-editor": "^3.0.2",
    "dotenv": "^16.4.5",
    "eventsource-parser": "^1.0.0",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "katex": "^0.16.21",
    "next-themes": "^0.4.4",
    "openai": "^4.87.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.0.0",
    "react-syntax-highlighter": "^15.6.0",
    "rehype-highlight": "^7.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-smartypants": "^3.0.2",
    "semver": "^7.7.1",
    "tailwindcss": "^4.0.8"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "parserPreset": "conventional-changelog-atom",
    "formatter": "@commitlint/format",
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "foo"
        ]
      ]
    },
    "defaultIgnores": true,
    "helpUrl": "https://github.com/conventional-changelog/commitlint/#what-is-commitlint",
    "prompt": {
      "messages": {},
      "questions": {
        "type": {
          "description": "please input type:"
        }
      }
    }
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/react": "^19.0.10",
    "@vitejs/plugin-react": "^4.3.4",
    "css-loader": "^6.7.3",
    "cypress": "^13.7.2",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.37.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.2.2",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.2.3",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.6",
    "prop-types": "^15.8.1",
    "semantic-release": "^24.2.3",
    "style-loader": "^4.0.0",
    "typescript": "^5.8.2",
    "url-loader": "^4.1.1",
    "vite": "^6.1.1"
  }
}
