    "react-icons": "^5.5.0",
    "react-markdown": "^10.0.0",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "^2.15.1",
    "rehype-highlight": "^7.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
